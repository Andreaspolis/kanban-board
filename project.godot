; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Kanban Board"
config/name_localized={
"et": "Kanbanitahvel"
}
config/description="Simple, modern and elegant Kanban board for organising your workflow"
config/version="0.1.0"
run/print_header=false
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/bg_color=Color(0.12549, 0.12549, 0.12549, 1)
config/icon="res://icons/icon.svg"

[display]

window/energy_saving/keep_screen_on=false
window/stretch/aspect="ignore"
window/handheld/orientation=4

[editor]

naming/default_signal_callback_name="_{node_name}_{signal_name}"
naming/default_signal_callback_to_self_name="_{signal_name}"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0.12549, 0.12549, 0.12549, 1)
